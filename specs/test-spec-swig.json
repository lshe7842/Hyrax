{
	"testCaseName": "Test case 1",

	"testCaseDescription": "This test case creates a home contents quote following happy path.",
	
	"url": "https://bats.insuranceonline.nrma.auiag.corp/oss/GTConnect/UnifiedAcceptor/Portal.Quote/brandId/nrma",
	
	"signalOk": "header.nrma",

	"numOfAsserts": 1,

	"expects": [
		"assertExists('select.optState')"
	],

	"actions": [
		{
			"type": "select(Home Insurance)",
			"form": "form",
			"target": ".optProductFamily",
			"after": {
				"signalOk": ".optProduct"
			}
		},

		{
			"type": "select(Home Contents)",
			"form": "form",
			"target": ".optProduct"
		},

		{
			"type": "click",
			"target": "button.btnGetQuote",
			"wait": 2,
			"after": {
				"waitFor": true,
				"signalOk": "div[id*=homeAddressGroup]",
				"print": "### Home quote page one loaded.",
				"timeout": 30,
				"timeoutMsg": "Timeout when proceeding to page one."
			}
		},

		{
			"type": "click",
			"target": "button.ui-datepicker-trigger",
			"after": {
				"signalOk": "table.ui-datepicker-calendar",
				"print": "### Datepicker initialized."
			}
		},

		{
			"type": "click",
			"target": "table.ui-datepicker-calendar .ui-datepicker-today a",
			"after": {
				"signalOk": "input[id*=cctDatePicker_datepicker]",
				"signalOkWith": "value(30/03/2016)",
				"print": "### Selected today's date from datepicker."
			}
		},

		{
			"type": "input(2223)",
			"focus": true,
			"target": "input[id*=autoCompleteStreet_input]",
			"after": {
				"signalOk": "ul#ui-id-1[style*='display: block']",
				"print": "### Found matched addresses"
			}
		},

		{
			"type": "click",
			"target": "ul#ui-id-1 > li",
			"after": {
				"signalOk": "ul#ui-id-1[style*='display: none']",
				"print": "### Selected address"
			}
		},

		{
			"type": "click",
			"target": "dd[id*=cctHomeOwner_dd0] label",
			"print": "### Selected 'Yes' for home owner."
		},

		{
			"type": "click",
			"target": "dd[id*=cctWhoLivesInHome_dd0] label",
			"print": "### Selected 'Me/Family'."
		},

		{
			"type": "click",
			"target": "dd[id*=cctBusinessOrPrivateUse_dd0] label",
			"print": "### Selected 'Private use' for home usage."
		},

		{
			"type": "click",
			"target": "label[id*=radStructureTightYes_label]",
			"print": "### Selected 'Yes' for home structure."
		},

		{
			"type": "click",
			"target": "label[id*=radUnderConstNo_label]",
			"print": "### Selected 'No' for home construction."
		},

		{
			"type": "click",
			"target": "label[id*=radRenoNo_label]",
			"print": "### Selected 'No' for home renovation."
		},

		{
			"type": "click",
			"target": "div[id*=plusMinusQuestions_0] a.plus-minus-button-plus",
			"print": "### Selected 1."
		},

		{
			"type": "click",
			"target": "label[id*=chkAgree_label]",
			"print": "### Agreed ToC."
		},

		{
			"type": "click",
			"target": "a[id*=btnContinue]",
			"after": {
				"waitFor": true,
				"signalOk": "div[id*=ContentBlockRooms]",
				"print": "### Landed on page two.",
				"timeout": 30,
				"timeoutMsg": "Timeout when proceeding to page three."
			}
		},

		{
			"type": "click",
			"target": "div[id*=fhdBedroomPlusMinus] a.plus-minus-button-plus",
			"print": "### Selected 1 bedroom."
		},

		{
			"type": "click",
			"target": "div[id*=fhdBathroom] a.plus-minus-button-plus",
			"print": "### Selected 1 bathroom."
		},

		{
			"type": "click",
			"target": "a[id*=btnContinue]",
			"after": {
				"waitFor": true,
				"signalOk": "div[id*=fhdPropertyType]",
				"print": "### Landed on page three.",
				"timeout": 30,
				"timeoutMsg": "Timeout when proceeding to page three."
			}
		},

		{
			"type": "click",
			"target": "label[id*=radHous_label]",
			"after": {
				"signalOk": "div[id*=contents_1]",
				"signalOkWith": "offsetHeight !== 0",
				"print": "### Selected 'Free-standing house' for property type."
			}
		},

		{
			"type": "click",
			"target": "label[id*=radElevatedNo_label]",
			"print": "### Selected 'No' to house elevation question."
		},

		{
			"type": "click",
			"target": "label[id*=radNoAlarm_label]",
			"print": "### Selected 'No Alarm'."
		},

		{
			"type": "click",
			"target": "a[id*=btnContinue]",
			"after": {
				"waitFor": true,
				"signalOk": "div[id*=fhdPolicyHolder]",
				"print": "### Landed on page four.",
				"timeout": 30,
				"timeoutMsg": "Timeout when proceeding to page three."
			}
		},

		{
			"type": "click",
			"target": "label[id*=radmyName_label]",
			"after": {
				"signalOk": "dl[id*=howManyOwners]",
				"signalOkWith": "offsetHeight !== 0",
				"print": "### Selected 'My name'."
			}
		},

		{
			"type": "click",
			"target": "dl[id*=howManyOwners] a.plus-minus-button-plus",
			"after": {
				"signalOk": "div[id*=fhdFirstOwnerDetails]",
				"signalOkWith": "offsetHeight !== 0",
				"print": "### Selected 1 owner."
			}
		},

		{
			"type": "click",
			"target": "label[id*=radExistingCustomerNo_label]",
			"after": {
				"signalOk": "div[id*=fhdCustomerInfo]",
				"signalOkWith": "offsetHeight !== 0",
				"print": "### Selected 'No' to existing customer question."
			}
		},

		{
			"type": "click",
			"target": "label[id*=radMale_label]",
			"print": "### Selected 'Male'."
		},

		{
			"type": "select(1)",
			"form": "form",
			"target": ".optDobDay",
			"print": "### Selected '1'."
		},

		{
			"type": "select(Jan)",
			"form": "form",
			"target": ".optDobMonth",
			"print": "### Selected 'Jan'."
		},

		{
			"type": "select(1990)",
			"form": "form",
			"target": "input[id*=txtDobYear]",
			"print": "### Typed '1990'."
		},

		{
			"type": "select(0)",
			"form": "form",
			"target": "input[id*=txtYrsPolicyHeld]",
			"print": "### Typed '0'."
		},

		{
			"type": "click",
			"target": "label[id*=radRefusedCancelledNo_label]",
			"print": "### Selected 'No' to insurance refused/cancelled question."
		},

		{
			"type": "click",
			"target": "a[id*=btnContinue]",
			"after": {
				"waitFor": true,
				"signalOk": "div.lblValuables",
				"print": "### Landed on page five.",
				"timeout": 30,
				"timeoutMsg": "Timeout when proceeding to page five."
			}
		},

		{
			"type": "click",
			"target": "div[id*=fhdCollections] label[id*=radNo_label]",
			"print": "### Selected 'No' to collections."
		},

		{
			"type": "click",
			"target": "div[id*=fhdWorksOfArt] label[id*=radNo_label]",
			"print": "### Selected 'No' to works of art."
		},

		{
			"type": "click",
			"target": "div[id*=fhdMusicMoviesGames] label[id*=radNo_label]",
			"print": "### Selected 'No' to music, movives and games."
		},

		{
			"type": "click",
			"target": "div[id*=fhdJewelleryWatches] label[id*=radNo_label]",
			"after": {
				"signalOk": "div.radNoPortable",
				"signalOkWith": "offsetHeight !== 0",
				"print": "### Selected 'No' to jewellery and watches."
			}
		},

		{
			"type": "click",
			"target": "label[id*=radNoPortable_label]",
			"print": "### Selected 'No' to portable jewellery and watches."
		},

		{
			"type": "click",
			"target": "div[id*=fhdElectronicDevices] label[id*=radNo_label]",
			"print": "### Selected 'No' to electronic devices."
		},

		{
			"type": "click",
			"target": "div[id*=fhdSportingRecreation] label[id*=radNo_label]",
			"print": "### Selected 'No' to sporting and recreation."
		},

		{
			"type": "click",
			"target": "div[id*=fhdGlassesMedical] label[id*=radNo_label]",
			"print": "### Selected 'No' to glasses and medical."
		},

		{
			"type": "click",
			"target": "div[id*=fhdClothingLuggage] label[id*=radNo_label]",
			"print": "### Selected 'No' to clothing and luggage."
		},

		{
			"type": "click",
			"target": "div[id*=fhdMobilityScooters] label[id*=radNo_label]",
			"print": "### Selected 'No' to mobility scooters."
		},

		{
			"type": "click",
			"target": "a[id*=btnContinue]",
			"after": {
				"waitFor": true,
				"signalOk": "div.fhdHomeStartDate",
				"print": "### Landed on summary page.",
				"timeout": 30,
				"timeoutMsg": "Timeout when creating a quote."
			}
		},

		{
			"type": "click",
			"target": "button[id*=btnContinue]",
			"after": {
				"waitFor": true,
				"signalOk": "div.single-quote-page-pricing",
				"timeout": 30,
				"timeoutMsg": "Timeout when creating a quote.",
				"prints": ["##### Page extracts #####", {
						"string": "The quote premium is: ",
						"query": "p.home-price-page__premium-val"
					}]
			}
		}
	]
}