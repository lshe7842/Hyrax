{
	"testCaseName": "Test case 1",

	"testCaseDescription": "This test case proceeds to motor quote page one and select a vehicle variant",
	
	"url": "https://sysa.insuranceonline.nrma.auiag.corp/oss/GTConnect/UnifiedAcceptor/Portal.Quote/brandId/nrma",
	
	"signalOk": "header.nrma",

	"numOfAsserts": 2,

	"expects": [
		"assertExists('select.optState')"
	],

	"actions": [
		{
			"type": "select(Car Insurance)",
			"form": "form",
			"target": ".optProductFamily"
		},
		{
			"type": "select(Comprehensive)",
			"form": "form",
			"target": ".optProduct",
			"wait": 3
		},
		{
			"type": "click",
			"target": "button.btnGetQuote",
			"wait": 3,
			"after": {
				"signalOk": ".top-vehicle-search",
				"expects": [
					"assertExists('select.optCarYear')"
				],
				"timeout": 6,
				"timeoutMsg": "Loading motor quote page one timeout."
			}
		},
		{
			"type": "select(2015)",
			"form": "form",
			"target": ".optCarYear",
			"wait": 3
		},
		{
			"type": "select(Audi)",
			"form": "form",
			"target": ".optCarMake",
			"wait": 3
		},
		{
			"type": "select(A3)",
			"form": "form",
			"target": ".optCarModel",
			"wait": 3
		},
		{
			"type": "select(Sedan)",
			"form": "form",
			"target": ".optCarShape",
			"wait": 3
		},
		{
			"type": "select(2.0)",
			"form": "form",
			"target": ".optCarEngineSize",
			"wait": 3,
			"after": {
				"signalOk": ".cctVehicleSearchResults",
				"print": ["#### Vehicle variants found.", {
					"query": ".cctVehicleSearchResults dd[id*=cctVehicleSearchResults_dd0] label",
					"string": "Vehicle 1: {?}"
				}, {
					"query": ".cctVehicleSearchResults dd[id*=cctVehicleSearchResults_dd0] label",
					"string": "Vehicle 1: {?}"
				}],
				"timeout": 10,
				"timeoutMsg": "Searching vehicle variants timeout."
			}
		}
	]
}